{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "moduleResolution": "node",
        "inlineSourceMap": true,
        "outDir": "./out",
        "noImplicitAny": false,
        "removeComments": false,
        "preserveConstEnums": true,
        "forceConsistentCasingInFileNames": true
    },
    "lib": [
        "es6",
        "es6-promise"
    ]
}