// A launch configuration that compiles the extension and then opens it inside a new window
{
	"version": "0.1.0",
	"configurations": [
		{
			"name": "Attach",
			"type": "node",
			"request": "attach",
			"address":"localhost",
			"port": 5555,
			"sourceMaps": true,
			"outDir": "${workspaceRoot}/../client/server"
		}
	]
}